{
  "extends": ["airbnb", "prettier", "plugin:vue/essential", "@vue/airbnb"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "no-return-assign": "off"
  },
  "root": true,
  "env": {
    "node": true
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}
